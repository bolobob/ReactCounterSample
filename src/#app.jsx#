var React = require('react');

var CounterApp = React.createClass({
  getInitialState: function() {
    return { counter: 0 };
  },
  handlePlus: function() {
    this.setState({ counter: this.state.counter + 1 });
  },
  handleMinus: function() {
    this.setState({ counter: this.state.counter - 1 });
  },
  render: function() {
    return (
      <d
  }
});
